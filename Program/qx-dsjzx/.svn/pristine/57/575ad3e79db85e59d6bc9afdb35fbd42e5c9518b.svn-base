    public class DataMigrationInfoDto
    {

        /// <summary>
        /// 地址链接
        /// </summary>
        public string HostLink { get; set; }
        /// <summary>
        /// 数据库名称
        /// </summary>
        public string DbName { get; set; }
        /// <summary>
        /// 数据库密码
        /// </summary>
        public string DbPwd { get; set; }
        /// <summary>
        /// 端口
        /// </summary>
        public string Port { get; set; }
        /// <summary>
